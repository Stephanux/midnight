 <div class="tree-container">
    <h1>Sitemap Rendering</h1>
    <div id="tree-root" class="tree">
        <ul>
            <li>
                <div class="endpoint"><h3>Site Index</h3></div>
                <ul>
                    <li>
                        {{> endpoint map }}
                    </li>
                
                </ul>
            </li>
        </ul>
    </div>
</div>
<div id="menus-container" class="menus">
    <!-- menu pour les types de retours -->
    <div id="types">
        <h3>Return Types</h3>
        <ul id="type-choices">
        {{#each types}}
            <li >{{this}}</li>
        {{/each}}
        </ul>
    </div>
</div>