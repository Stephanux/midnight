
{{#if childRoutes}}
    <ul>  
    {{#each childRoutes}}
        <li>
            <div class="endpoint">
                <h3>{{@key}}</h3>
                <a href="./delete/{{../__uuid__}}/{{__uuid__}}">DELETE</a>
                {{> methods this}}
            </div>
            {{> endpoint this}}
        </li>
    {{/each}}
        <li>
            <a href="./addendpoint/{{__uuid__}}" class="round-btn"><i class="material-icons">add</i></a>
        </li>
    </ul>
{{else}}
    <ul>
        <li>
            <a href="./addendpoint/{{__uuid__}}" class="round-btn"><i class="material-icons">add</i></a>
        </li>
    </ul>
{{/if}}
    