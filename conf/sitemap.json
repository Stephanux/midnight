{
	"GET": {
		"view": "index",
		"type":"json",
		"extra":"jesaispas"
	},
	"PUT": {
		"view": "putter",
		"type":"csv"
	},
	"childRoutes": {
		"exos": {
			"GET": {
				"view": "exos",
				"type":"simplexml",
				"url_params":{
					"truc":{
						"isInt":{
							"options":[{
								"gt":10,
								"lt":100
							}],
							"errorMessage":"Must be between 10 and 100 inclusive"
						}
						
					}
				}
			},
			"childRoutes": {
				":id": {
					"GET": {
						"params": {
							"id":{
								"isInt":{
									"options":{
										"min":0,
										"max":100
									},
									"errorMessage":"Must be between 0 and 100 exclusive!"
								}
							}	
						},
						"url_params":{
								"name":{
									"optional":true,
									"matches":{
										"options":["(A|B)-[a-z]{6}-[A-F0-9]{4}"]
									},
									"errorMessage":"Invalid name format"
								}
					},
						"view": "exos_details",
						"type":"json"
					}
				}
			}
		},
		"users/:id/:name": {
			"GET": {
				"params": {
					"id":{
						"isInt":{
							"options":{
								"min":0,
								"max":100
							}
						}
					},
					"name":{
						"isLength":{
							"min":3							
						},
						"matches":{
							"pattern":"(A|B)-[a-z]{6}-[A-F0-9]{4}"
						}
					}
				},
				"view": "list_user",
				"type":"simplexml"
			},
			"PUT": {
				"view": "update_user",
				"type":"simplexml"
			}
		},
		"dogs/:hello/:mail": {
			"GET": {
				
				"type":"json"
			}
		},
		"admin": {
			"GET": {
				"type":"simplexml"
				},
			"childRoutes": {
				"stats/:type": {
					"GET": {
						
						"type":"json"
					}
				}
			}
		}
	}
}