{
	"GET": {
		"view": "index",
		"type":"json",
		"extra":"jesaispas"
	},
	"PUT": {
		"view": "putter",
		"type":"csv"
	},
	"childRoutes": {
		"exos": {
			"GET": {
				"view": "exos",
				"type":"simplexml",
				"url_params":[
					{
						"name":"truc",
						"type":"int",
						"min":0,
						"max":100
					}
				]
			},
			"childRoutes": {
				":id": {
					"GET": {
						"params": [
							{
								"name": "id",
								"type": "int"
							}
						],
						"url_params":[
							{
								"name":"orderby",
								"type":"string",
								"pattern":"^h[eE]l{2,3}o$"
							}
						],
						"view": "exos_details",
						"type":"json"
					}
				}
			}
		},
		"users/:id/:name": {
			"GET": {
				"params": [
					{
						"name": "id",
						"type": "int",
						"min": 0,
						"max": 100,
						"null": false,
						"default_value": 0
					},
					{
						"name": "name",
						"type": "string",
						"minLength": 5,
						"maxLength": 14,
						"pattern": "(A|B)-[a-z]{6}-[A-F0-9]{4}",
						"null": false
					}
				],
				"view": "list_user",
				"type":"simplexml"
			},
			"PUT": {
				"view": "update_user",
				"type":"simplexml"
			}
		},
		"dogs/:hello/:mail": {
			"GET": {
				"params": [
					{
						"name": "hello",
						"type": "custom",
						"validator": "my.validator",
						"null": false
					},
					{
						"name": "mail",
						"type": "email"
					}
				],
				"type":"json"
			}
		},
		"admin": {
			"GET": {
				"type":"simplexml"
				},
			"childRoutes": {
				"stats/:type": {
					"GET": {
						"params": [
							{
								"name": "type",
								"type": "int",
								"min": 0
							}
						],
					"type":"json"
					}
				}
			}
		}
	}
}